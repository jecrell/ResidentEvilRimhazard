    <resolvers>
      <li Class="RERimhazard.SymbolResolver_BasePart_Indoors_Leaf_GEncounter">
        <minRectSize>(8, 8)</minRectSize>
        <selectionWeight>1</selectionWeight>
      </li>
      <li Class="RERimhazard.SymbolResolver_BasePart_Indoors_Leaf_TyrantEncounter">
        <minRectSize>(8, 8)</minRectSize>
        <selectionWeight>1</selectionWeight>
      </li>
      <li Class="RERimhazard.SymbolResolver_BasePart_Indoors_Leaf_ZombieDogEncounter">
        <minRectSize>(6, 4)</minRectSize>
        <selectionWeight>1</selectionWeight>
      </li>
      <li Class="RERimhazard.SymbolResolver_BasePart_Indoors_Leaf_ZombieEncounter">
        <minRectSize>(3, 3)</minRectSize>
        <selectionWeight>1</selectionWeight>
      </li>
      <li Class="RERimhazard.SymbolResolver_BasePart_Indoors_Leaf_LickerEncounter">
        <minRectSize>(5, 5)</minRectSize>
        <selectionWeight>0.4</selectionWeight>
      </li>
      <li Class="RERimhazard.SymbolResolver_BasePart_Indoors_Leaf_CrimsonHeadEncounter">
        <minRectSize>(5, 5)</minRectSize>
        <selectionWeight>0.05</selectionWeight>
      </li>
    </resolvers>
  </RuleDef>


