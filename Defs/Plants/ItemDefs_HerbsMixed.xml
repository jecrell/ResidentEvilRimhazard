<Defs>


  <ThingDef ParentName="ResourceBase" Name="RE_IngestibleHerbs" Abstract="True">
    <thingClass>RERimhazard.CombinableHerb</thingClass>
    <stackLimit>25</stackLimit>
    <uiIconForStackCount>1</uiIconForStackCount>
    <soundInteract>Standard_Drop</soundInteract>
    <soundDrop>Standard_Drop</soundDrop>
    <statBases>
      <MaxHitPoints>60</MaxHitPoints>
    </statBases>
    <thingCategories>
      <li>Medicine</li>
    </thingCategories>
    <ingestible>
      <preferability>NeverForNutrition</preferability>
      <maxNumToIngestAtOnce>1</maxNumToIngestAtOnce>
      <foodType>Plant, Processed</foodType>
      <outcomeDoers>
        <li Class="RERimhazard.IngestionOutcomeDoer_Herb">
        </li>
      </outcomeDoers>
    </ingestible>
  </ThingDef>

  <ThingDef ParentName="RE_IngestibleHerbs">
    <defName>RE_MedicineBlueBlueBlue</defName>
    <label>Mixed Herbs (B+B+B)</label>
    <description>.</description>
    <graphicData>
      <texPath>Things/Item/Resource/Herbs/BlueBlueBluePowder</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>0.85</drawSize>
    </graphicData>
		<techLevel>Neolithic</techLevel>
		<statBases>
      <MarketValue>18</MarketValue>
      <Mass>0.50</Mass>
      <Flammability>0.7</Flammability>
      <MedicalPotency>1.00</MedicalPotency>
      <MedicalQualityMax>1.00</MedicalQualityMax>
      <DeteriorationRate>2</DeteriorationRate>
      <WorkToMake>700</WorkToMake>
    </statBases>
    <recipeMaker>
			<recipeUsers>
        <li>CraftingSpot</li>
        <li>DrugLab</li>
      </recipeUsers>
      <workSpeedStat>DrugCookingSpeed</workSpeedStat>
      <workSkill>Cooking</workSkill>
    </recipeMaker>
    <costList>
      <RE_MedicineBlue>3</RE_MedicineBlue>
    </costList>
  </ThingDef>

